    <sql>SELECT e.patient_id, 
e.encounter_date as visit_date,
e.location,
CASE WHEN (neuroPVD + deformity + footUlcer) = 3 THEN 1 end as foot_exam
from omrs_encounter e 
LEFT JOIN (SELECT encounter_id, 1 AS neuroPVD from omrs_obs where concept = 'Neuropathy and Peripheral Vascular Disease') neuroPVD ON neuroPVD.encounter_id = e.encounter_id
LEFT JOIN (SELECT encounter_id, 1 AS deformity from omrs_obs where concept = 'Deformity of foot') deformity ON deformity.encounter_id = e.encounter_id
LEFT JOIN (SELECT encounter_id, 1 AS footUlcer from omrs_obs where concept = 'Foot ulcer or infection') footUlcer ON footUlcer.encounter_id = e.encounter_id
WHERE e.encounter_type = 'DIABETES HYPERTENSION FOLLOWUP'
group by 	e.patient_id, e.encounter_date, e.location
order by 	patient_id, visit_date, location;</sql>
