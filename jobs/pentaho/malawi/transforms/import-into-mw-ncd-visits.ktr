    <name>Import Mental Status Exam</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>Warehouse</connection>
    <sql>SELECT e.patient_id, 
e.encounter_date as visit_date,
e.location,
CASE WHEN (depressive + hypomania + disrupt + abnormal + delusion + hallucinate + lackInsight + other) = 8 THEN 'Normal' end as mental_status_exam
from omrs_encounter e 
LEFT JOIN (SELECT encounter_id, 1 AS depressive from omrs_obs where concept = 'Mental health chief complaint absent' AND value_coded = "Depressive Disorder") depressive ON depressive.encounter_id = e.encounter_id
LEFT JOIN (SELECT encounter_id, 1 AS hypomania from omrs_obs where concept = 'Mental health chief complaint absent' AND value_coded = "Hypomania") hypomania ON hypomania.encounter_id = e.encounter_id
LEFT JOIN (SELECT encounter_id, 1 AS disrupt from omrs_obs where concept = 'Mental health chief complaint absent' AND value_coded = "Disruptive Behavior Disorder") disrupt ON disrupt.encounter_id = e.encounter_id
LEFT JOIN (SELECT encounter_id, 1 AS abnormal from omrs_obs where concept = 'Mental health chief complaint absent' AND value_coded = "Abnormal speech") abnormal ON abnormal.encounter_id = e.encounter_id
LEFT JOIN (SELECT encounter_id, 1 AS delusion from omrs_obs where concept = 'Mental health chief complaint absent' AND value_coded = "Delusions") delusion ON delusion.encounter_id = e.encounter_id
LEFT JOIN (SELECT encounter_id, 1 AS hallucinate from omrs_obs where concept = 'Mental health chief complaint absent' AND value_coded = "Hallucinations") hallucinate ON hallucinate.encounter_id = e.encounter_id
LEFT JOIN (SELECT encounter_id, 1 AS lackInsight from omrs_obs where concept = 'Mental health chief complaint absent' AND value_coded = "Lack of insight") lackInsight ON lackInsight.encounter_id = e.encounter_id
LEFT JOIN (SELECT encounter_id, 1 AS other from omrs_obs where concept = 'Mental health chief complaint absent' AND value_coded = "Other non-coded") other ON other.encounter_id = e.encounter_id
WHERE e.encounter_type = 'MENTAL_HEALTH_FOLLOWUP'
group by 	e.patient_id, e.encounter_date, e.location
order by 	patient_id, visit_date, location;</sql>
