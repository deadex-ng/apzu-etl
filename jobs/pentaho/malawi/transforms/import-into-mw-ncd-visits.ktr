			group_concat(distinct e.encounter_type order by encounter_type asc SEPARATOR ', ') as visit_types
from        omrs_encounter e
where 		e.encounter_type in (
  'CHRONIC_CARE_INITIAL',
  'CHRONIC_CARE_FOLLOWUP',
