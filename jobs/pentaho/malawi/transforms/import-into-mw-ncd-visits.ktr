  'CKD_FOLLOWUP',
  'CHF_INITIAL',
  'CHF_FOLLOWUP',
  'NCD_OTHER_INITIAL',
  'NCD_OTHER_FOLLOWUP'
  
)
group by 	e.patient_id, e.encounter_date, e.location
order by 	patient_id, visit_date, location
